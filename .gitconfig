[user]
	name = Douglas Price
	email = doug.price@solipsys.com
[alias]
	stll = log --graph --pretty=oneline --decorate --abbrev-commit
	lol = log --graph --decorate --pretty=oneline --abbrev-commit
	lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
[core]
	autocrlf = false
[push]
	default = upstream
[branch]
	autosetuprebase = always
[merge]
	tool = kdiff3
[mergetool "kdiff3"]
	path = c:/Program Files/KDiff3/kdiff3.exe
[diff]
	guitool = kdiff3
[difftool "kdiff3"]
	path = c:/Program Files/KDiff3/kdiff3.exe
[color]
	ui = auto
