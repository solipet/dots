[alias]
  co    = checkout
  ci    = commit
  cm    = commit -m
  ca    = commit --amend
  laf   = log --abbrev-commit --decorate --date=short --format=format:'%C("#ffff00")%h%C(reset) - %C(cyan)%ad%C(reset) %C("#008800")(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset) %C("#aa55ff")%D%C(reset)' --all --full-history --
  lola  = log --graph --abbrev-commit --decorate --date=short --format=format:'%C("#ffff00")%h%C(reset) - %C(cyan)%ad%C(reset) %C("#008800")(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset) %C("#aa55ff")%D%C(reset)' --all
  lb    = log --graph --abbrev-commit --decorate --date=short --format=format:'%C("#ffff00")%h%C(reset) - %C(cyan)%ad%C(reset) %C("#008800")(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset) %C("#aa55ff")%D%C(reset)'
  pop   = stash pop
  dev   = checkout develop
  stg   = checkout staging
  main  = checkout main
  sin   = push --force-with-lease
  satan = push --force
  unpull = reset --hard HEAD@{1}
[user]
  email = douglas.price@gsa.gov
  name = Douglas Price
[core]
  excludesfile = /Users/doug/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path =
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[rebase]
  instructionFormat = [%an @ %ar] %s
[grep]
  lineNumber = true
